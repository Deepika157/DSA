iterative:
#include <iostream>
using namespace std;

int factorial(int n)
{
	int r = 1;
	for (int i = 2; i <= n; i++)
		r*= i;
	return r;
}

int main()
{
	int num;
  cin>>num;
	cout << "Factorial is "<< factorial(num) << endl;
	return 0;
}


recursive:

#include <iostream>
using namespace std;

int factorial( int n)
{
	if (n == 0 || n == 1)
		return 1;
    
	return n * factorial(n - 1);
}

int main()
{
	int num;
  cin>>num;
	cout << "Factorial  is " << factorial(num) << endl;
	return 0;
}


